	<rdfs:domain rdf:resource="http://www.eclipse.org/epf/uma/1.0.6/uma.ecore#RoleDescriptor"/>
	<rdfs:range rdf:resource="http://www.eclipse.org/epf/uma/1.0.6/uma.ecore#WorkProduct"/>
